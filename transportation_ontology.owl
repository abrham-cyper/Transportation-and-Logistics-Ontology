<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/transportation-logistics-ontology#"
     xml:base="http://www.semanticweb.org/transportation-logistics-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/transportation-logistics-ontology">
        <rdfs:comment>An ontology for transportation and logistics concepts.</rdfs:comment>
        <rdfs:label>Transportation and Logistics Ontology</rdfs:label>
        <owl:versionInfo>1.1</owl:versionInfo>
    </owl:Ontology>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#carries -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#carries">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Cargo"/>
        <rdfs:comment>Links a vehicle to the cargo it carries.</rdfs:comment>
        <rdfs:label>carries</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#consumes -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#consumes">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Fuel"/>
        <rdfs:comment>Type of fuel the vehicle uses.</rdfs:comment>
        <rdfs:label>consumes</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#destination -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#destination">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#TransportProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Location"/>
        <rdfs:comment>Where the transport ends.</rdfs:comment>
        <rdfs:label>has destination</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#hauls -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#hauls">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#TransportProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle"/>
        <rdfs:comment>The vehicle used for the transport.</rdfs:comment>
        <rdfs:label>hauls</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#origin -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#origin">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#TransportProcess"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Location"/>
        <rdfs:comment>Where the transport starts.</rdfs:comment>
        <rdfs:label>has origin</rdfs:label>
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#fuelCapacity -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#fuelCapacity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Max fuel in liters.</rdfs:comment>
        <rdfs:label>fuel capacity</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#weight -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#weight">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Cargo"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Weight in kilograms.</rdfs:comment>
        <rdfs:label>weight</rdfs:label>
    </owl:DatatypeProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Cargo -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Cargo">
        <rdfs:comment>Items being transported.</rdfs:comment>
        <rdfs:label>Cargo</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Fuel -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Fuel">
        <rdfs:comment>Energy source for the vehicle.</rdfs:comment>
        <rdfs:label>Fuel</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Location -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Location">
        <rdfs:comment>A geographic place.</rdfs:comment>
        <rdfs:label>Location</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Port -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Port">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Location"/>
        <rdfs:comment>A place for ships to dock.</rdfs:comment>
        <rdfs:label>Port</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Warehouse -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Warehouse">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Location"/>
        <rdfs:comment>Building for storing goods.</rdfs:comment>
        <rdfs:label>Warehouse</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#TransportProcess -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#TransportProcess">
        <rdfs:comment>Moving goods from one place to another.</rdfs:comment>
        <rdfs:label>Transport Process</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Vehicle -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle">
        <rdfs:comment>Machine for transport.</rdfs:comment>
        <rdfs:label>Vehicle</rdfs:label>
    </owl:Class>

    <!-- http://www.semanticweb.org/transportation-logistics-ontology#Truck -->
    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Truck">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle"/>
        <rdfs:comment>Vehicle for transporting goods on roads.</rdfs:comment>
        <rdfs:label>Truck</rdfs:label>
    </owl:Class>


    <owl:Class rdf:about="http://www.semanticweb.org/transportation-logistics-ontology#Ship">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/transportation-logistics-ontology#Vehicle"/>
        <rdfs:comment>Boat for sea transport.</rdfs:comment>
        <rdfs:label>Ship</rdfs:label>
    </owl:Class>

</rdf:RDF>
